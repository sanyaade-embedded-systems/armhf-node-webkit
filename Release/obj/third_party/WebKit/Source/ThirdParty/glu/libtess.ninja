defines = -D_FILE_OFFSET_BITS=64 -DCHROMIUM_BUILD $
    -DUSE_DEFAULT_RENDER_THEME=1 -DUSE_LIBJPEG_TURBO=1 -DUSE_NSS=1 $
    -DENABLE_ONE_CLICK_SIGNIN -DGTK_DISABLE_SINGLE_INCLUDES=1 $
    -DENABLE_REMOTING=1 -DENABLE_WEBRTC=1 -DUSE_PROPRIETARY_CODECS $
    -DENABLE_PEPPER_THREADING -DENABLE_CONFIGURATION_POLICY $
    -DENABLE_INPUT_SPEECH -DENABLE_NOTIFICATIONS -DENABLE_GPU=1 $
    -DENABLE_EGLIMAGE=1 -DUSE_SKIA=1 -DENABLE_TASK_MANAGER=1 $
    -DENABLE_WEB_INTENTS=1 -DENABLE_EXTENSIONS=1 $
    -DENABLE_PLUGIN_INSTALLATION=1 -DENABLE_PLUGINS=1 $
    -DENABLE_SESSION_SERVICE=1 -DENABLE_THEMES=1 -DENABLE_BACKGROUND=1 $
    -DENABLE_AUTOMATION=1 -DENABLE_GOOGLE_NOW=1 $
    -DENABLE_LANGUAGE_DETECTION=1 -DENABLE_PRINTING=1 $
    -DENABLE_CAPTIVE_PORTAL_DETECTION=1 -DNDEBUG -DNVALGRIND $
    -DDYNAMIC_ANNOTATIONS_ENABLED=0 -DUSE_EABI_HARDFLOAT
includes = -I../../third_party/WebKit/Source/ThirdParty/glu
cflags = -fstack-protector --param=ssp-buffer-size=4 -pthread $
    -fno-exceptions -fno-strict-aliasing -Wno-unused-parameter $
    -Wno-missing-field-initializers -fvisibility=hidden -pipe -fPIC $
    -Wno-format -Wno-unused-result -march=armv7-a -mtune=cortex-a8 $
    -mfloat-abi=hard -mfpu=neon -O2 -fno-ident -fdata-sections $
    -ffunction-sections
cflags_c = 
cflags_cc = -fno-rtti -fno-threadsafe-statics -fvisibility-inlines-hidden $
    -Wno-deprecated -Wno-abi

build obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.dict.o: $
    cc ../../third_party/WebKit/Source/ThirdParty/glu/libtess/dict.c
build obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.geom.o: $
    cc ../../third_party/WebKit/Source/ThirdParty/glu/libtess/geom.c
build $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.memalloc.o: $
    cc ../../third_party/WebKit/Source/ThirdParty/glu/libtess/memalloc.c
build obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.mesh.o: $
    cc ../../third_party/WebKit/Source/ThirdParty/glu/libtess/mesh.c
build $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.normal.o: $
    cc ../../third_party/WebKit/Source/ThirdParty/glu/libtess/normal.c
build $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.priorityq.o: $
    cc ../../third_party/WebKit/Source/ThirdParty/glu/libtess/priorityq.c
build $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.render.o: $
    cc ../../third_party/WebKit/Source/ThirdParty/glu/libtess/render.c
build obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.sweep.o: $
    cc ../../third_party/WebKit/Source/ThirdParty/glu/libtess/sweep.c
build obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.tess.o: $
    cc ../../third_party/WebKit/Source/ThirdParty/glu/libtess/tess.c
build $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.tessmono.o: $
    cc ../../third_party/WebKit/Source/ThirdParty/glu/libtess/tessmono.c

build obj/third_party/WebKit/Source/ThirdParty/glu/libtess.a: alink_thin $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.dict.o $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.geom.o $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.memalloc.o $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.mesh.o $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.normal.o $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.priorityq.o $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.render.o $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.sweep.o $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.tess.o $
    obj/third_party/webkit/source/thirdparty/glu/libtess/libtess.tessmono.o
